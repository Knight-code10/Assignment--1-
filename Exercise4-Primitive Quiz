quiz1 = input("What is the capital of France?: ")

if quiz1 == "Paris":
        print("Your are correct.")
elif quiz1 == "paris":
    print("Your are correct")
elif quiz1 == "PaRis":
    print("Your are correct")
else:
    print("Your are wrong.")

